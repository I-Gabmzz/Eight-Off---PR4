<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controladores.ControladorHistorial">
    <center>
        <ListView fx:id="listaMovimientosView" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </BorderPane.margin>
        </ListView>
    </center>
    <bottom>
        <HBox alignment="CENTER" prefHeight="60.0" spacing="20.0" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="botonRetroceder" mnemonicParsing="false" onAction="#retroceder" prefWidth="120.0" text="&lt; Retroceder">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Button>
                <Button fx:id="botonAvanzar" mnemonicParsing="false" onAction="#avanzar" prefWidth="120.0" text="Avanzar &gt;">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Button>
                <Button fx:id="botonAplicar" mnemonicParsing="false" onAction="#aplicarCambios" prefWidth="120.0" style="-fx-background-color: #aeffb4; -fx-border-color: #006400;" text="Aplicar Estado">
                    <font>
                        <Font name="System Bold" size="14.0" />
                    </font>
                </Button>
                <Button fx:id="botonCerrar" cancelButton="true" mnemonicParsing="false" onAction="#cerrarSinCambios" prefWidth="120.0" style="-fx-background-color: #ffb4b4; -fx-border-color: #8b0000;" text="Cerrar">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Button>
            </children>
        </HBox>
    </bottom>
</BorderPane>